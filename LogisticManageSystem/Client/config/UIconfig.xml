<?xml version="1.0" encoding="UTF-8"?>
<presentation>
	<Frame name = "MainFrame" class = "MainFrame" width = "1080" height = "720">
		<mainPanel name = "MainPanel" class = "MainPanel" x="0" y="0"  w = "1080" h = "720">
			<component name="background" class = "Component" x="0" y="0" w="1080" h="720"/>
			<component name="username" class = "Component" x="620" y="270" w="78" h="25"/>
			<component name="password" class = "Component" x="620" y="360" w="78" h="25"/>
			<component name="loginback" class = "Component" x="560" y="120" w="400" h="500"/>
			<unit name ="SignIn" class = "MainButton" x="685" y="500" w="150" h="32" type="1" listenerName="SignInButtonListener"/>
			<unit name ="userName" class = "MyTextField" x="715" y="270" w="168" h="23" type="0"/>
			<unit name ="password" class = "MyPasswordField" x="715" y="360" w="168" h="23" type="0"/>
			<unit name ="loginButton" class = "MainButton" x="560" y="60" w="100" h="60" type="1" listenerName="LogInListener"/>
			<unit name ="inquireButton" class = "MainButton" x="660" y="60" w="100" h="60" type="0"/>
		</mainPanel>
		<mainPanel name = "InquirePanel" class = "MainPanel" x="0" y="0"  w = "1080" h = "720">
			<component name="background" class = "Component" x="0" y="0" w="1080" h="720"/>
			<component name="loginback" class = "Component" x="560" y="120" w="400" h="500"/>
			<unit name ="loginButton" class = "MainButton" x="560" y="60" w="100" h="60" type="0"/>
			<unit name ="inquireButton" class = "MainButton" x="660" y="60" w="100" h="60" type="1" listenerName="InquireListener"/>
		</mainPanel>
		<mainPanel name="ManagerPanel" class="MainPanel" x="0" y="0"  w = "1080" h = "720">
		    <component name="background" class = "Component" x="0" y="0" w="1080" h="720"/>
		    <unit name ="PersonnelManage" class = "MainButton" x="200" y="100" w="128" h="128" type="1" listenerName="PersonnelManageButtonListener"/>
		</mainPanel>
		<mainPanel name="PersonnelManagePanel" class="MainPanel" x="0" y="0"  w = "1080" h = "720">
		     <component name="background" class = "Component" x="0" y="0" w="1080" h="720"/>
		     <component name="PersonnelManage" class = "Component" x="30" y="20" w="98" h="23"/>
		     <unit name ="mainTable" class = "MainTable" x="100" y="100" w="900" h="460" type="2" dataType = "UserVO" rw = "23" cw = "168" model = "PersonnelTableModel"/>
		     <unit name ="addPerson" class = "MainButton" x="50" y="60" w="150" h="32" type="1" listenerName="AddPersonButtonListener"/>
		     <unit name ="delete" class = "MainButton" x="600" y="600" w="150" h="32" type="1" listenerName="DeletePersonButtonListener"/>
		     <unit name ="save" class = "MainButton" x="780" y="600" w="150" h="32" type="1" listenerName="SavePersonButtonListener"/>
		     <unit name ="findPerson" class = "MainButton" x="800" y="60" w="53" h="20" type="1" listenerName="FindPersonButtonListener"/>
		     <unit name ="userName" class = "MyTextField" x="575" y="60" w="168" h="20" type="0"/>
		</mainPanel>
		<mainPanel name="AddPersonPanel" class="MainPanel" x="0" y="0" w="1080" h="720">
		    <component name="background" class = "Component" x="0" y="0" w="1080" h="720"/>
		    <component name="personName" class = "Component" x="50" y="50" w="78" h="23"/>
		    <component name="personDuty" class = "Component" x="50" y="100" w="78" h="23"/>
		    <component name="institution" class = "Component" x="50" y="150" w="78" h="23"/>
		    <component name="salary" class = "Component" x="50" y="200" w="78" h="23"/>
		    <unit name ="personID" class = "MyTextField" x="130" y="50" w="168" h="23" type="0"/>
		    <unit name ="personName" class = "MyTextField" x="130" y="100" w="168" h="23" type="0"/>
		    <unit name ="institution" class = "MyTextField" x="130" y="150" w="168" h="23" type="0"/>
		    <unit name ="salary" class = "MyTextField" x="130" y="200" w="168" h="23" type="0" />
		    <unit name ="perTime" class = "MyTextField" x="130" y="250" w="168" h="23" type="0" />
		    <unit name ="bonus" class = "MyTextField" x="130" y="300" w="168" h="23" type="0" />
		    <unit name ="personDuty" class = "MyCombox" x="130" y="350" w="168" h="23" type="0" items="总经理;高级财务人员;普通财务人员;中转中心业务员;营业厅业务员;仓库管理人员;快递员"/>
		    <unit name ="confirm" class = "MainButton" x="130" y="400" w="75" h="16" type="1" listenerName="AddPersonConfirmListener"/>
		    <unit name ="cancel" class = "MainButton" x="220" y="400" w="75" h="16" type="1" listenerName="CancelAddListener"/>
		</mainPanel>
	</Frame>
</presentation>